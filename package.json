{
    "name": "custom-bracket-select",
    "displayName": "Custom Bracket Select",
    "description": "Quick select code between brackets, support for (), {}, [], '' and \"\" (Fork of jhasse.bracket-select2)",
    "version": "2.2.0",
    "publisher": "ycookiey",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.34.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ycookiey/vscode-bracket-select.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:bracket-select.select",
        "onCommand:bracket-select.select-include"
    ],
    "extensionKind": [
        "ui"
    ],
    "main": "./out/src/bracketSelectMain",
    "contributes": {
        "commands": [
            {
                "command": "bracket-select.select",
                "title": "BraSel:Select"
            },
            {
                "command": "bracket-select.select-include",
                "title": "BraSel:SelectInclude"
            }
        ],
        "keybindings": [
            {
                "command": "bracket-select.select",
                "key": "alt+a",
                "mac": "alt+a",
                "when": "editorTextFocus"
            },
            {
                "command": "bracket-select.select-include",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a",
                "when": "editorTextFocus"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Bracket Select Configuration",
            "properties": {
                "bracket-select.brackets": {
                    "type": "array",
                    "default": [
                        [
                            "(",
                            ")"
                        ],
                        [
                            "{",
                            "}"
                        ],
                        [
                            "[",
                            "]"
                        ]
                    ],
                    "description": "Array of bracket pairs to support."
                },
                "bracket-select.quotes": {
                    "type": "array",
                    "default": [
                        "\"",
                        "'",
                        "`"
                    ],
                    "description": "Array of quote characters to support."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.34.0",
        "tslint": "^5.19.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {}
}
